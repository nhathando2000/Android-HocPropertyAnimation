<?xml version="1.0" encoding="utf-8"?>
<set xmlns:android="http://schemas.android.com/apk/res/android"
    android:ordering="sequentially">
    <objectAnimator
        android:propertyName="X"
        android:duration="3000"
        android:valueTo="200">
    </objectAnimator>
    <objectAnimator
        android:propertyName="Y"
        android:duration="3000"
        android:valueTo="500">
    </objectAnimator>
    <objectAnimator
        android:propertyName="X"
        android:duration="3000"
        android:valueTo="0">
    </objectAnimator>
    <objectAnimator
        android:propertyName="Y"
        android:duration="3000"
        android:valueTo="100">
    </objectAnimator>

</set>